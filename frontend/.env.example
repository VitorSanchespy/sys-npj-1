# 📝 Frontend Environment Variables
# Configure as variáveis abaixo conforme seu ambiente

# 🌐 API Backend URLs
VITE_API_URL=http://localhost:3001
VITE_API_BASE_URL=http://localhost:3001/api

# 🔧 Ambiente de Desenvolvimento
VITE_NODE_ENV=development

# 🔒 Configurações de Segurança (opcional)
# VITE_ENABLE_HTTPS=false
# VITE_HTTPS_CERT_PATH=
# VITE_HTTPS_KEY_PATH=

# 🎯 Configurações de Build (opcional)
# VITE_BUILD_TARGET=es2015
# VITE_SOURCEMAP=true

# 🚀 Configurações de Performance (opcional)
# VITE_CHUNK_SIZE_LIMIT=500
# VITE_OPTIMIZE_DEPS=true
