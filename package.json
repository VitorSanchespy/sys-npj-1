{
  "name": "sistema-npj",
  "version": "1.0.0",
  "description": "Sistema de Gestão do Núcleo de Prática Jurídica",
  "main": "backend/index.js",
  "scripts": {
    "setup": "node setup-database.js",
    "setup:win": "setup.bat",
    "setup:unix": "./setup.sh",
    "start": "start.bat",
    "start:win": "start.bat",
    "start:unix": "./start.sh",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "test": "npm run test:structure && npm run test:backend",
    "test:structure": "node bdScripts/test-database-structure.js",
    "test:backend": "node test-endpoints-complete.js",
    "test:complete": "node test-system-complete.js",
    "test:all": "node run-all-tests.js",
    "test:api": "cd backend && npm test",
    "test:e2e": "cd frontend && npx cypress run",
    "test:e2e:open": "cd frontend && npx cypress open",
    "test:agendamentos": "cd backend && npm run test:agendamentos",
    "clean": "node cleanup.js",
    "migrate": "cd backend && npm run migrate",
    "db:reset": "cd backend && npm run db:reset",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test:frontend": "node test-frontend-e2e.js",
    "test:setup": "node setup-test-environment.js"
  },
  "keywords": [
    "npj",
    "juridico",
    "gestao",
    "processos",
    "nodejs",
    "react",
    "mysql"
  ],
  "author": "Sistema NPJ",
  "license": "MIT",
  "dependencies": {
    "@mantine/notifications": "^8.1.3",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.3",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cypress": "^13.7.0",
    "jest": "^29.7.0",
    "playwright": "^1.54.2",
    "puppeteer": "^24.16.1",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4",
    "vite": "^7.0.2"
  }
}
