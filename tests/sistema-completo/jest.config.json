{
  "testEnvironment": "node",
  "testMatch": [
    "**/tests/sistema-completo/**/*.spec.js"
  ],
  "collectCoverage": true,
  "coverageDirectory": "tests/sistema-completo/reports/coverage",
  "coverageReporters": [
    "text",
    "lcov",
    "html",
    "json"
  ],
  "testTimeout": 30000,
  "setupFilesAfterEnv": [
    "<rootDir>/setup.js"
  ],
  "verbose": true
}